import json,sys

def featureRatings(all_ratings,location):
	ratings = []	
	for cluster in all_ratings[location]:
		ratings.append(cluster[0][0]+('\t %.2f')%(cluster[1]))		
	#print(ratings)		
	return ratings
	
if __name__ == "__main__":
	with open('featurerating.txt') as infile:
		all_ratings = json.load(infile)
	location = 'aquatica'
	rating = featureRatings(all_ratings,location)
	print(str(rating))



